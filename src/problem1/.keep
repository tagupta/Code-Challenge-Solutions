//using AP
var sum_to_n_a = function(n) {
   return n*(n+1) / 2 ; 
};

//using iteration
var sum_to_n_b = function(n) {
    var sum = 0;
    for(var i = 1 ; i <= n ; i++){
      sum += i;
    }
    return sum;  
};

//using recursion
var sum_to_n_c = function(n) {
    if(n == 1) return 1;
    else return n + sum_to_n_c(n-1);
 };